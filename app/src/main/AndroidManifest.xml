<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.renameExample.android.learnChinese">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.renameExample.android.learnChinese.NumbersActivity"
            android:label="@string/category_numbers"
            android:parentActivityName="com.renameExample.android.learnChinese.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.renameExample.android.learnChinese.MainActivity"/>
        </activity>
        <activity
            android:name="com.renameExample.android.learnChinese.FamilyActivity"
            android:label="@string/category_family"/>
        <activity
            android:name="com.renameExample.android.learnChinese.ColorsActivity"
            android:label="@string/category_colors"/>
        <activity
            android:name="com.renameExample.android.learnChinese.PhrasesActivity"
            android:label="@string/category_phrases"/>

        <activity android:name="com.renameExample.android.learnChinese.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>